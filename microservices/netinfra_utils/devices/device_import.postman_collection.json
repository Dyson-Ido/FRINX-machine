{
	"info": {
		"_postman_id": "48827c7d-a287-41f5-8b59-6c2a3eacd1bc",
		"name": "Default device import",
		"schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
	},
	"item": [
		{
			"name": "Import devices",
			"request": {
				"method": "POST",
				"header": [
					{
						"key": "Content-Type",
						"name": "Content-Type",
						"value": "application/json",
						"type": "text"
					}
				],
				"body": {
					"mode": "raw",
					"raw": "{\n\t\"device_type\": \"{{device_type}}\",\n\t\"device_version\": \"{{device_version}}\",\n\t\"host\": \"sample-topology\",\n\t\"id\": \"{{device_id}}\",\n\t\"password\": \"{{password}}\",\n\t\"username\": \"{{username}}\",\n\t\"port\": \"{{port}}\",\n\t\"transport_type\": \"ssh\",\n\t\"labels\" : [\"{{labels}}\"]\n}"
				},
				"url": {
					"raw": "http://elasticsearch:9200/inventory-device/device/{{device_id}}",
					"protocol": "http",
					"host": [
						"elasticsearch"
					],
					"port": "9200",
					"path": [
						"inventory-device",
						"device",
						"{{device_id}}"
					]
				}
			},
			"response": []
		}
	]
}
